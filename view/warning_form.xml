<form col="6">
    <label name="timestamp"/>
    <field name="timestamp"/>
    <label name="table_to_show"/>
    <field name="table_to_show"/>
    <label name="count"/>
    <field name="count"/>
    <label name="group"/>
    <field name="group"/>
    <label name="user"/>
    <field name="user"/>
    <label name="company"/>
    <field name="company"/>
    <label name="employee"/>
    <field name="employee"/>
    <label name="party"/>
    <field name="party"/>
    <label name="done_by"/>
    <field name="done_by"/>
    <label name="ignored_by"/>
    <field name="ignored_by"/>
    <notebook colspan="6">
        <page name="preview">
            <field name="preview" colspan="4" height="450" widget="document"/>
        </page>
        <page name="description">
            <field name="description" colspan="4" height="450"/>
        </page>
    </notebook>
    <label name="state"/>
    <field name="state"/>
    <group colspan="4" id="state">
        <button name="pending"/>
        <button name="do"/>
        <button name="ignore"/>
        <button name="open"/>
    </group>
</form>
