<form col="6">
    <label name="name"/>
    <field name="name"/>
    <label name="internal_name"/>
    <field name="internal_name"/>
    <label name="type"/>
    <field name="type"/>
    <label name="model"/>
    <field name="model"/>
    <label name="filter"/>
    <field name="filter"/>
    <button name="compute" colspan="2"/>
    <newline/>
    <notebook colspan="6">
        <page name="query">
            <field name="query" colspan="4" height="550"/>
        </page>
        <page name="fields_">
            <field name="fields_" colspan="4" height="550"/>
        </page>
        <page name="preview">
            <field name="preview" colspan="4" widget="document" height="550"/>
        </page>
        <page name="crons">
            <field name="crons" colspan="4"/>
        </page>
        <page name="compute_error">
            <field name="compute_error" colspan="4"/>
        </page>
        <page id="configuration" string="Configuration" col="6">
            <label name="preview_limit"/>
            <field name="preview_limit"/>
            <label name="babi_raise_user_error"/>
            <field name="babi_raise_user_error"/>
            <label name="timeout"/>
            <field name="timeout"/>
	</page>
        <page id="dependencies" string="Dependencies">
            <field name="requires" colspan="2" height="550"/>
            <field name="required_by" colspan="2" height="550"/>
        </page>
    </notebook>
</form>
