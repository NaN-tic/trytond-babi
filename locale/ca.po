msgctxt "model:ir.message,text:filter_parameters"
msgid ""
"Execution \"%(execution)s\" has filter parameters\n"
"          and you did not provide any of them. Please execute it.\n"
"          from the menu."
msgstr ""
"L'execució \"%(execution)s\" té paràmetres de filtre\n"
"           i vostè no va proporcionar cap d'ells. Si us plau executa-ho\n"
"           des del menú."

msgctxt "model:ir.message,text:no_dimensions"
msgid ""
"Report \"%(report)s\" has no dimensions. At least\n"
"                    one is needed."
msgstr ""
"L'informe \"%(report)s\" no té dimensions. Al menys\n"
"                     un és necessari."

msgctxt "model:ir.message,text:no_execution"
msgid ""
"No execution found for current record.\n"
"          Execute the update data wizard in order to create one."
msgstr ""
"No s’ha trobat cap execució per al registre actual.\n"
"           Executeu l’assistent de dades d’actualització per crear-ne un."

msgctxt "model:ir.message,text:no_measures"
msgid "Report \"%s\" has no measures. At least one is needed."
msgstr "L’informe \"%s\" no té cap mesura. Almenys un és necessari."

msgctxt "model:ir.message,text:no_menus"
msgid ""
"No menus found for report %(report)s. In order to view\n"
"                    'it\\'s data you must create menu entries."
msgstr ""
"No s'han trobat menús per a l'informe %(report)s. per veure\n"
"           les dades ha de crear entrades de menú."

msgctxt "model:ir.message,text:no_report"
msgid "No report found for current execution"
msgstr "No s’ha trobat cap informe per a l’execució actual"

msgctxt "model:ir.message,text:one_measure_in_pie_charts"
msgid "Only one measure can be used in pie charts"
msgstr "Només es pot utilitzar una mesura en gràfics circulars"

msgctxt "model:ir.message,text:parameter_not_found"
msgid ""
"Parameter \"%(parameter)s\" not found '\n"
"                    'in Domain nor in Python Expression of filter '\n"
"                    '\"%(filter)s\"."
msgstr ""
"Paràmetre \"%(parameter)s\" no trobat '\n"
"                     'al dominio ni a la expessió Python del filtre'\n"
"                     '\"%(filter)s\"."

msgctxt "model:ir.message,text:report_not_exists"
msgid ""
"Report \"%(report)s\" no longer exists or you do\n"
"                    not have the rights to access it."
msgstr ""
"L’informe \"%(report)s\" ja no existeix o \n"
"                     no teniu els permissos d'accés."

msgctxt "model:ir.message,text:timeout_exception"
msgid ""
"Report \"%(report)s\" will be renamed,\n"
"                    but if you want to also rename the menus you should\n"
"                    create the menus again. Are you sure you want to\n"
"                    continue with this action?"
msgstr ""
"L'informe \"%(report)s\" serà reanomenat,\n"
"                     però si vols també canviar el nom dels menús hauries de\n"
"                     crear els menús de nou. Segur que vols\n"
